{
  "extends": "tslint-config-shopify",
  "rules": {
    "no-require-imports": false,
    "no-console": false,
    "member-ordering": [
      false
    ],
    "quotemark": false,
    "trailing-comma": false,
    "curly": false,
    "space-before-function-paren": false,
    "no-empty": false,
    "one-line": false
  }
}